    <DTS:Executable
      DTS:refId="Package\Обновление статей в OLAP Л-Фото"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Обновление статей в OLAP Л-Фото"
      DTS:DTSID="{A97CBCD0-A1F3-492E-B61D-0C9EFDBD8E21}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Обновление статей в OLAP Л-Фото"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{9931CF9D-A548-48EC-A4BE-5E2BDEF98AD8}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="DECLARE @nullreference binary(16), @SND binary(16), @SNDR binary(16), &#xA;@SKD binary(16), @SKDR binary(16), @SNK binary(16), @SNKR binary(16), &#xA;&#xA;@SKK binary(16), @SKKR binary(16), @DO binary(16), @KO binary(16), &#xA;@KorDO binary(16), @KorKO binary(16), @DOKO binary(16), @KorDOKorKO &#xA;binary(16)&#xA;SET              @nullreference = 0x00000000000000000000000000000000&#xA;SET              @SND = 0x99780BAE4D539E634BE54556157DB615&#xA;SET              @SNDR = 0x9BA678A8868ED49B424778DD639E7B10&#xA;SET              @SKD = 0x8A798FB014192D244CC0389C7E611888&#xA;SET              @SKDR = 0x97F60F4278F163D14C7FB20CB07D7602&#xA;SET              @SNK = 0x8380467117DD2342482313E95CDA4986&#xA;SET              @SNKR = 0x88FAA58C64EBAB174CA900B177F3EE45&#xA;SET              @SKK = 0xB933B5562841A7AF464A361820495CD2&#xA;SET              @SKKR = 0xA48CCC724A3F8F7E40988DE7F61CD1F0&#xA;SET              @DO = 0xB84E3CE7F9807BC847AF892B4E35D1A3&#xA;SET &#xA;             @KO = 0x9E77EE1C962568E2435A90B4FB67A5A6&#xA;SET  &#xA;            @KorDO = 0x81D1BF13D74BDCD143449E58D05C548D&#xA;SET &#xA;             @KorKO = 0x992B04D91757D8664C591B671C764CAC&#xA;SET &#xA;             @DOKO = 0xB58890F01ABE784041FA0B3A0741CCD6&#xA;SET &#xA;             @KorDOKorKO = 0xB1E53E1DD11B08844D70E829C131AD4B&#xA;                          &#xA;DELETE &#xA;FROM FinStats&#xA;                                             &#xA;         INSERT      &#xA;                                     &#xA;                  &#xA;INTO            FinStats&#xA;                                  &#xA;                                                &#xA;SELECT     ISNULL(ФинСтатьиВОтчетах_1.Код, '00000000') + '00000' &#xA;ParentID, ФинСтатьиВОтчетах.Код + '00000' ID, &#xA;            &#xA;                                                            &#xA;                                  ФинСтатьиВОтчетах.Наименование &#xA;Descr, @nullreference schet, ФинСтатьиВОтчетах.КодОтчета otchet, &#xA;&#xA;                                                          &#xA;                                                CASE LTRIM(RTRIM(ФинСтатьиВОтчетах.Знак)) &#xA;WHEN '' THEN '+' ELSE ФинСтатьиВОтчетах.Знак END&#xA;          &#xA;                                                            &#xA;             &#xA;FROM         LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах &#xA;ФинСтатьиВОтчетах LEFT &#xA;JOIN&#xA;                                                      &#xA;                                                    LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах &#xA;ФинСтатьиВОтчетах_1 ON &#xA;                                   &#xA;                                                            &#xA;           ФинСтатьиВОтчетах_1.Ссылка = ФинСтатьиВОтчетах.Родитель&#xA;                                                            &#xA;                       &#xA;WHERE     ФинСтатьиВОтчетах.ПометкаУдаления = 0&#xA;UNION&#xA;SELECT &#xA;    ФинСтатьиВОтчетах.Код + '00000' ParentID, ФинСтатьиВОтчетах.Код &#xA;+ RIGHT('0000' + CAST(ФинКоррСтатейВОтчетах.Код AS VARCHAR(5)), &#xA;5) ID, &#xA;                      CASE WHEN ФинКоррСтатейВОтчетах.Знак &#xA;&lt; 0 THEN '-' ELSE '' END + CASE ФинКоррСтатейВОтчетах.ВидСуммы &#xA;WHEN @SND THEN 'СНД ' WHEN&#xA;                       @SNDR THEN &#xA;'СНДР ' WHEN @SKD THEN 'СКД ' WHEN @SKDR THEN 'СКДР ' WHEN @SNK &#xA;THEN 'СНК ' WHEN @SNKR THEN 'СНКР ' WHEN @SKK&#xA;             &#xA;          THEN 'СКК ' WHEN @SKKR THEN 'СККР ' WHEN @DO THEN &#xA;'ДО ' WHEN @KO THEN 'КО ' WHEN @KorDO THEN 'КорДО ' WHEN @KorKO &#xA;THEN 'КорКО '&#xA;                       WHEN @DOKO THEN 'ДО-КО &#xA;&#xA;&#xA;' WHEN @KorDOKorKO THEN 'КорДО-КорКО ' ELSE '' END + RTRIM(LTRIM(ПланСчетов_Управленческий.Код))&#xA;                       + '-' + LTRIM(RTRIM(ISNULL(ПланСчетов_Управленческий_К.Код, &#xA;''))) + ' ' + LTRIM(RTRIM(ПланСчетов_Управленческий.Наименование)) &#xA;Descr, &#xA;                      ФинКоррСтатейВОтчетах.Счет schet,&#xA;                          (SELECT     КодОтчета&#xA;           &#xA;                 &#xA;FROM          LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах &#xA;ФинСтатьиВОтчетах&#xA;                            &#xA;WHERE      ФинСтатьиВОтчетах.Ссылка = ФинКоррСтатейВОтчетах.Владелец) &#xA;otchet, '+'&#xA;FROM         LSERVER63.s3_conv_full.dbo.Справочник_ФинКоррСтатейВОтчетах &#xA;ФинКоррСтатейВОтчетах INNER &#xA;JOIN&#xA;                      LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах &#xA;ФинСтатьиВОтчетах ON &#xA;                      ФинКоррСтатейВОтчетах.Владелец &#xA;= ФинСтатьиВОтчетах.Ссылка INNER &#xA;JOIN&#xA;                      LSERVER63.s3_conv_full.dbo.Перечисление_ФинВидыИтогов &#xA;ФинВидыИтогов ON &#xA;                      ФинВидыИтогов.Ссылка &#xA;= ФинКоррСтатейВОтчетах.ВидСуммы LEFT &#xA;JOIN&#xA;                      LSERVER63.s3_conv_full.dbo.ПланСчетов_Управленческий &#xA;ПланСчетов_Управленческий ON &#xA;                      ПланСчетов_Управленческий.Ссылка &#xA;= ФинКоррСтатейВОтчетах.Счет LEFT &#xA;JOIN&#xA;                      LSERVER63.s3_conv_full.dbo.ПланСчетов_Управленческий &#xA;ПланСчетов_Управленческий_К ON &#xA;                      ПланСчетов_Управленческий_К.Ссылка &#xA;= ФинКоррСтатейВОтчетах.КорСчет&#xA;WHERE     ФинКоррСтатейВОтчетах.ПометкаУдаления &#xA;= 0 AND ФинКоррСтатейВОтчетах.Активен = 1&#xA;ORDER BY ParentID&#xA;                          &#xA;DELETE &#xA;FROM FinPeriods&#xA;                                           &#xA;           INSERT      &#xA;                                   &#xA;                    &#xA;INTO            FinPeriods&#xA;                                &#xA;                                                  &#xA;SELECT     DATEADD(yy, - 2000, ФинПериодыОЛАП.начПериода), DATEADD(yy, &#xA;- 2000, &#xA;                                                  &#xA;                                                        ФинПериодыОЛАП.конПериода), &#xA;ФинПериодыОЛАП.Наименование, &#xA;                             &#xA;                                                            &#xA;                 ФинПериодыОЛАП.флОбновлять&#xA;               &#xA;                                                            &#xA;        &#xA;FROM         LSERVER63.s3_conv_full.dbo.Справочник_ФинПериодыОЛАП &#xA;ФинПериодыОЛАП&#xA;                                            &#xA;                                       &#xA;WHERE     ФинПериодыОЛАП.ПометкаУдаления = 0&#xA;              &#xA;                                                            &#xA;                                    &#xA;DELETE &#xA;FROM FinScena&#xA;                                             &#xA;                                                            &#xA;      &#xA;WHERE     ID &lt;&gt; @nullreference&#xA;                            &#xA;                                                            &#xA;                                                  INSERT    &#xA;  &#xA;                                                        &#xA;                                                            &#xA;                       &#xA;INTO            FinScena&#xA;                                  &#xA;                                                            &#xA;                                                            &#xA;            &#xA;SELECT     ФинСценарии.Ссылка, ФинСценарии.Родитель, ФинСценарии.Код, &#xA;&#xA;                                                          &#xA;                                                            &#xA;                                                            &#xA;            ФинСценарии.ПометкаУдаления&#xA;                   &#xA;                                                            &#xA;                                                            &#xA;                            &#xA;FROM         LSERVER63.s3_conv_full.dbo.Справочник_ФинСценарии &#xA;ФинСценарии" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
