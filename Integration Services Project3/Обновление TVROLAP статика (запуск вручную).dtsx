      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C6970D0F-CA8E-4785-89D8-FA86540B5FD2}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="Declare @DateOfUpdate Datetime, @BeginDate Datetime&#xA;SET @DateOfUpdate = dateadd(s,-1,Convert(datetime,'20180101',112))&#xA;SET @BeginDate = Convert(datetime,'20083101',112)&#xA;&#xA;declare @SP7150 binary(16),&#xA;&#x9;@PARENTID binary(16),&#xA;&#x9;@ID binary(16),&#xA;&#x9;@PCODE char(8),&#xA;&#x9;@PDESCR char(100),&#xA;&#x9;@PISFOLDER tinyint,&#xA;&#x9;@PISMARK binary(1),&#xA;&#x9;@PrType char(50),&#xA;&#x9;@PDirectID binary(16),&#xA;&#x9;@PDirectDD char(100),&#xA;&#x9;@BBrand char(30),&#xA;&#x9;@InPrice numeric(15,4),&#xA;&#x9;@BestPrice numeric(15,4),&#xA;        @PCODEPOSTAVSHIK char(50)&#xA;&#xA;declare @SP7150_ binary(16),&#xA;&#x9;@PARENTID_ binary(16),&#xA;&#x9;@ID_ binary(16),&#xA;&#x9;@PCODE_ char(8),&#xA;&#x9;@PDESCR_ char(100),&#xA;&#x9;@PISFOLDER_ tinyint,&#xA;&#x9;@PISMARK_ binary(1),&#xA;&#x9;@PrType_ char(50),&#xA;&#x9;@PDirectID_ binary(16),&#xA;&#x9;@PDirectDD_ char(100),&#xA;&#x9;@BBrand_ char(30),&#xA;&#x9;@InPrice_ numeric(15,4),&#xA;&#x9;@BestPrice_ numeric(15,4),&#xA;&#x9;@PCODEPOSTAVSHIK_ char(50)&#xA;&#xA;Declare @Brand binary(16)&#xA;declare Cur cursor LOCAL STATIC type_warning for&#xA;Select &#xA;&#x9;wares.ТипТоваров&#xA;&#x9;,wares.Товар&#xA;&#x9;,wares.Родитель&#xA;&#x9;,wares.Код&#xA;&#x9;,wares.Наименование&#xA;&#x9;,wares.ЭтоГруппа&#xA;&#x9;,wares.ПометкаУдаления&#xA;&#x9;,wares.ТипыСвойства&#xA;&#x9;,wares.Направление&#xA;&#x9;,wares.НаименованиеНаправление&#xA;&#x9;,wares.Бренд&#xA;&#x9;,wares.ЦенаОптовая&#xA;&#x9;,wares.СпецЦена&#xA;&#x9;,wares.КодПоставщика&#xA;&#x9;from&#xA;&#x9;&#x9;(Select &#xA;&#x9;&#x9;WaresSales.ТипТоваров&#xA;&#x9;&#x9;,WaresSales.Товар&#xA;&#x9;&#x9;,WaresSales.Родитель&#xA;&#x9;&#x9;,WaresSales.Код&#xA;&#x9;&#x9;,WaresSales.Наименование&#xA;&#x9;&#x9;,WaresSales.ЭтоГруппа&#xA;&#x9;&#x9;,WaresSales.ПометкаУдаления&#xA;&#x9;&#x9;,WaresSales.ТипыСвойства&#xA;&#x9;&#x9;,WaresSales.Направление&#xA;&#x9;&#x9;,WaresSales.НаименованиеНаправление&#xA;&#x9;&#x9;,WaresSales.Бренд&#xA;&#x9;&#x9;,WaresSales.ЦенаОптовая&#xA;&#x9;&#x9;,WaresSales.СпецЦена&#xA;&#x9;&#x9;,WaresSales.КодПоставщика&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;From(&#xA;&#x9;&#x9;select  &#xA;&#x9;&#x9;ISNULL(Номенклатура.ТипТоваров,0x00000000000000000000000000000000) as ТипТоваров &#xA;&#x9;&#x9;,[Товар] as Товар &#xA;&#x9;&#x9;, Номенклатура.Родитель as Родитель&#xA;&#x9;&#x9;, Номенклатура.Код as Код&#xA;&#x9;&#x9;, Номенклатура.Наименование as Наименование&#xA;&#x9;&#x9;, CASE WHEN Номенклатура.ЭтоГруппа = 0x01 THEN 2 ELSE 1 END as ЭтоГруппа&#xA;&#x9;&#x9;, Номенклатура.ПометкаУдаления as ПометкаУдаления&#xA;&#x9;&#x9;, ISNULL(ТипыСвойств.Наименование,'Прочее') as ТипыСвойства&#xA;&#x9;&#x9;, ISNULL(Направления.Ссылка,0x00000000000000000000000000000000) as Направление&#xA;&#x9;&#x9;, ISNULL(Направления.Наименование,'Прочее') as НаименованиеНаправление&#xA;&#x9;&#x9;, ISNULL(Бренды.Наименование,'Прочее') as Бренд&#xA;&#x9;&#x9;, ISNULL((SELECT TOP 1 Цена&#xA;&#x9;&#x9;&#x9;FROM LSERVER63.s3_conv_full.dbo.РегистрСведений_ЦеныНоменклатуры&#xA;&#x9;&#x9;&#x9;WHERE Номенклатура = Номенклатура.Ссылка&#xA;&#x9;&#x9;&#x9;AND КатегорияЦены = 0x20202020203443202000000000000000--(SELECT Ссылка FROM LSERVER63.s3_conv_full.dbo.Справочник_КатегорииЦен WHERE Наименование = 'Оптовая') &#xA;&#x9;&#x9;&#x9;AND Период &lt;= DATEADD(YY,2000,@DateOfUpdate)&#xA;&#x9;                                &#xA;&#x9;&#x9;&#x9;ORDER BY Период desc),0) as ЦенаОптовая&#xA;                  &#xA;&#x9;&#x9;, ISNULL((SELECT TOP 1 Цена&#xA;&#x9;&#x9;&#x9;FROM LSERVER63.s3_conv_full.dbo.РегистрСведений_ЦеныНоменклатуры&#xA;&#x9;&#x9;&#x9;WHERE Номенклатура = Номенклатура.Ссылка&#xA;&#x9;&#x9;&#x9;AND КатегорияЦены = 0x20202020204147202000000000000000--(SELECT Ссылка FROM LSERVER63.s3_conv_full.dbo.Справочник_КатегорииЦен WHERE Наименование = 'СпецЦена') &#xA;&#x9;&#x9;&#x9;AND Период &lt;= DATEADD(YY,2000,@DateOfUpdate)&#xA;&#x9;&#x9;&#x9;ORDER BY Период desc),0)   as СпецЦена&#xA;&#x9;&#x9; , ISNULL(Номенкла