              <properties>
                <property
                  dataType="System.Int32"
                  description="Время ожидания команды, в секундах. Величина 0 означает неограниченное время ожидания."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Указывает имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Указывает переменную, содержащую имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">declare @UpdtDate datetime, @EDate datetime
set @UpdtDate = Convert(datetime,'2017-12-31 23:59:59.000',121)

SELECT 
	   CAST(LEFT(CONVERT(CHAR(8), DATEADD(YEAR, -2000, туПродажи.Период), 112), 8) AS DATETIME) DATE_TIME_IDDOC, 
       CAST(ISNULL(LEFT(CONVERT(CHAR(8), DATEADD(YEAR, -2000, РасходнаяНакладная.Дата), 112), 8), LEFT(CONVERT(CHAR(8), DATEADD(YEAR, -2000, туПродажи.Период), 112), 8)) AS DATETIME) DATE_SHIP, 
       CONVERT(NVARCHAR(32), туПродажи.Контрагент, 2) AS SP413
       , --Kln 
       CONVERT(NVARCHAR(32), туПродажи.Менеджер, 2) AS SP7182
       ,  --Mnd 
       CONVERT(NVARCHAR(32), туПродажи.Товар, 2) AS SP423
       , --Tov 
       SUM(туПродажи.КолВо) SP424
       , --Kol 
       SUM(туПродажи.Оборот) SP428
       ,--Summ 
       CONVERT(NVARCHAR(32), туПродажи.Автор, 2) AS SP1006
       , --Avt	  
       1 VidDv
       , --_22        
       CONVERT(NVARCHAR(32), туПродажи.Склад, 2) Store,
       CASE Склады.База
           WHEN 1
           THEN 'Ярмарка'
           WHEN 2
           THEN 'Березка'
           WHEN 4
           THEN 'Рябина'
           WHEN 5
           THEN 'Рябина'
           WHEN 9
           THEN 'Горбушка'
           WHEN 3
           THEN 'И-Магазин'
           ELSE 'Прочее'
       END Office
       , --_33 
       CONVERT(NVARCHAR(32), туПродажи.Направление, 2) Direction
       , --_44     
       SUM(туПродажи.Сумма) Sebestoimost
       , --SP8537 
       CONVERT(NVARCHAR(32), туПродажи.ЦФО, 2) SP11818, 
       CONVERT(NVARCHAR(32), Товары.ТипТоваров, 2) SP7150
       , 
       --добавлено 20-11-2018 
       '00000000000000000000000000000000' AS MngNSS
       , --_44     
       SUM(туПродажи.ОборотРуб) ProdazyRUR, 
       SUM(туПродажи.СуммаРуб) SebestoimostRUR,
       CASE КаналыПродаж.Код
           WHEN 'G0015'
           THEN 1
           ELSE 0
       END NSS, 
       '00000000000000000000000000000000' AS RegPredst
       , --_44 
       MAX(ISNULL(РегистрСведений_КлиентыПланПродаж.СуммаРасчетная, 0)) AS Plan_Sales_Calculated, 
       MAX(ISNULL(РегистрСведений_КлиентыПланПродаж.[СуммаМенеджер], 0)) AS Plan_Sales_Manager, 
       MAX(ISNULL(РегистрСведений_КлиентыПланПродаж.[СуммаКанал], 0)) AS Plan_Sales_Channel, 
       MAX(ISNULL(РегистрСведений_КлиентыПланПродаж.[СуммаДиректор], 0)) AS Plan_Sales_Dierctor, 
       MAX(ISNULL(РегистрСведений_КлиентыПланПродаж.[СуммаПотенциал], 0)) AS Plan_Sales_Potential
FROM РегистрНакопления_туПродажи(NOLOCK) туПродажи
     INNER JOIN Справочник_МестаХранения Склады(NOLOCK) ON Склады.Ссылка = туПродажи.Склад
     INNER JOIN [dbo].[Справочник_Контрагенты] Справочник_Контрагенты(NOLOCK) ON Справочник_Контрагенты.Ссылка = туПродажи.Контрагент
     INNER JOIN Справочник_Номенклатура Товары(NOLOCK) ON Товары.Ссылка = туПродажи.Товар
     LEFT JOIN [dbo].Справочник_ТипыСвойств Справочник_ТипыТовар(NOLOCK) ON Справочник_ТипыТовар.Ссылка = Товары.ТипТоваров
     LEFT JOIN Справочник_Клиенты Клиенты(NOLOCK) ON Клиенты.Ссылка = туПродажи.Клиент
     LEFT JOIN Справочник_КаналыПродаж КаналыПродаж(NOLOCK) ON КаналыПродаж.Ссылка = Клиенты.ТекущийКаналПродаж
     LEFT JOIN Документ_РасходнаяНакладная РасходнаяНакладная(NOLOCK) ON РасходнаяНакладная.Ссылка = туПродажи.ДокПродажи
     LEFT JOIN [dbo].[РегистрСведений_КлиентыПланПродаж] РегистрСведений_КлиентыПланПродаж(NOLOCK) ON(РегистрСведений_КлиентыПланПродаж.[Клиент] = туПродажи.[Клиент]
                                                                                                      AND РегистрСведений_КлиентыПланПродаж.[Направление] = туПродажи.Направление
                                                                                                      AND CAST(РегистрСведений_КлиентыПланПродаж.Период AS DATETIME) = CAST(LEFT(CONVERT(CHAR(8), DATEADD(DAY, 1 - DAY(туПродажи.[Период]), туПродажи.[Период]), 112), 8) AS DATETIME)
                                                                                                      AND РегистрСведений_КлиентыПланПродаж.ТоварнаяГруппа = Справочник_ТипыТовар.[ТоварнаяГруппа]
                                                                                                      AND РегистрСведений_КлиентыПланПродаж.Бренд = Товары.Бренд)
WHERE CONVERT(char(8),DATEADD(YY,-2000,туПродажи.Период),112) &lt;= @UpdtDate

      AND Склады.ВидСклада &lt;&gt; 0xA7C7A872D0A0A6924C9AC05F5DAF35A2
      AND Товары.Наименование IS NOT NULL --Есть битые ссылки

GROUP BY CAST(LEFT(CONVERT(CHAR(8), DATEADD(YEAR, -2000, туПродажи.Период), 112), 8) AS DATETIME), 
         ISNULL(LEFT(CONVERT(CHAR(8), DATEADD(YEAR, -2000, РасходнаяНакладная.Дата), 112), 8), LEFT(CONVERT(CHAR(8), DATEADD(YEAR, -2000, туПродажи.Период), 112), 8)), 
         туПродажи.ДокПродажи, 
         туПродажи.Контрагент, 
         туПродажи.Менеджер, 
         туПродажи.Товар, 
         туПродажи.ДокПродажи, 
         туПродажи.Автор, 
         туПродажи.Склад, 
         туПродажи.Направление,
         CASE Склады.База
             WHEN 1
             THEN 'Ярмарка'
             WHEN 2
             THEN 'Березка'
             WHEN 4
             THEN 'Рябина'
             WHEN 5
             THEN 'Рябина'
             WHEN 9
             THEN 'Горбушка'
             WHEN 3
             THEN 'И-Магазин'
             ELSE 'Прочее'
         END, 
         туПродажи.ЦФО, 
         Товары.ТипТоваров,
         CASE КаналыПродаж.Код
             WHEN 'G0015'
             THEN 1
             ELSE 0
         END, 
         КаналыПродаж.РуководительКанала
ORDER BY DATE_TIME_IDDOC DESC;</property>
