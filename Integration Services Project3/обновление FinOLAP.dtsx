    <DTS:Executable
      DTS:refId="Package\Обновление статей"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Задача &quot;Выполнение SQL&quot;"
      DTS:DTSID="{35A83847-EA67-48AD-BA34-97D59D407FD1}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Обновление статей"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{86F53F65-77A1-4C33-9A09-5BC3911176CA}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="Declare @nullreference binary(16),&#xA;        @SND binary(16),&#xA;&#x9;&#x9;@SNDR binary(16),&#xA;&#x9;&#x9;@SKD binary(16),&#xA;&#x9;&#x9;@SKDR binary(16),&#xA;&#x9;&#x9;@SNK binary(16),&#xA;&#x9;&#x9;@SNKR binary(16),&#xA;&#x9;&#x9;@SKK binary(16),&#xA;&#x9;&#x9;@SKKR binary(16),&#xA;&#x9;&#x9;@DO binary(16),&#xA;&#x9;&#x9;@KO binary(16),&#xA;&#x9;&#x9;@KorDO binary(16),&#xA;&#x9;&#x9;@KorKO binary(16),&#xA;&#x9;&#x9;@DOKO binary(16),&#xA;&#x9;&#x9;@KorDOKorKO binary(16)&#xA;&#xA;SET&#x9;&#x9;@nullreference = 0x00000000000000000000000000000000 -- пустая ссылка&#xA;SET&#x9;&#x9;@SND&#x9;&#x9;   = 0x99780BAE4D539E634BE54556157DB615 -- 'СНД '&#xA;SET&#x9;&#x9;@SNDR          = 0x9BA678A8868ED49B424778DD639E7B10 -- 'СНДР '&#xA;SET&#x9;&#x9;@SKD           = 0x8A798FB014192D244CC0389C7E611888 -- 'СКД '&#xA;SET&#x9;&#x9;@SKDR          = 0x97F60F4278F163D14C7FB20CB07D7602 -- 'СКДР '&#xA;SET&#x9;&#x9;@SNK           = 0x8380467117DD2342482313E95CDA4986 -- 'СНК '&#xA;SET&#x9;&#x9;@SNKR          = 0x88FAA58C64EBAB174CA900B177F3EE45 -- 'СНКР '&#xA;SET&#x9;&#x9;@SKK           = 0xB933B5562841A7AF464A361820495CD2 -- 'СКК '&#xA;SET&#x9;&#x9;@SKKR          = 0xA48CCC724A3F8F7E40988DE7F61CD1F0 -- 'СККР '&#xA;SET&#x9;&#x9;@DO            = 0xB84E3CE7F9807BC847AF892B4E35D1A3 -- 'ДО '&#xA;SET&#x9;&#x9;@KO            = 0x9E77EE1C962568E2435A90B4FB67A5A6 -- 'КО '&#xA;SET&#x9;&#x9;@KorDO         = 0x81D1BF13D74BDCD143449E58D05C548D -- 'КорДО '&#xA;SET&#x9;&#x9;@KorKO         = 0x992B04D91757D8664C591B671C764CAC -- 'КорКО '&#xA;SET&#x9;&#x9;@DOKO          = 0xB58890F01ABE784041FA0B3A0741CCD6 -- 'ДО-КО '&#xA;SET&#x9;&#x9;@KorDOKorKO    = 0xB1E53E1DD11B08844D70E829C131AD4B -- 'КорДО-КорКО '&#xA;&#xA;DELETE FROM FinStats&#xA;&#xA;Insert INTO FinStats&#xA;SELECT ISNULL(ФинСтатьиВОтчетах_1.Код,'00000000')+'00000' ParentID&#xA;     , ФинСтатьиВОтчетах.Код+'00000' ID&#xA;     , ФинСтатьиВОтчетах.Наименование Descr&#xA;     , @nullreference schet&#xA;     , ФинСтатьиВОтчетах.КодОтчета otchet&#xA;     , Case LTRIM(RTRIM(ФинСтатьиВОтчетах.Знак)) WHEN '' THEN '+'ELSE ФинСтатьиВОтчетах.Знак END&#xA;FROM LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах ФинСтатьиВОтчетах&#xA;LEFT JOIN LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах ФинСтатьиВОтчетах_1 ON ФинСтатьиВОтчетах_1.Ссылка=ФинСтатьиВОтчетах.Родитель&#xA;WHERE ФинСтатьиВОтчетах.ПометкаУдаления = 0&#xA;&#xA;UNION&#xA;&#xA;SELECT ФинСтатьиВОтчетах.Код+'00000' ParentID&#xA;    ,  ФинСтатьиВОтчетах.Код + RIGHT('0000'+CAST(ФинКоррСтатейВОтчетах.Код AS VARCHAR(5)),5) ID&#xA;    ,  Case When ФинКоррСтатейВОтчетах.Знак&lt;0 Then '-' Else '' END +&#xA;&#x9;   Case ФинКоррСтатейВОтчетах.ВидСуммы When @SND THEN 'СНД '&#xA;&#x9;&#x9;&#x9;When  @SNDR THEN 'СНДР '&#xA;&#x9;&#x9;&#x9;When  @SKD THEN 'СКД '&#xA;&#x9;&#x9;&#x9;When  @SKDR THEN 'СКДР '&#xA;&#x9;&#x9;&#x9;When  @SNK THEN 'СНК '&#xA;&#x9;&#x9;&#x9;When  @SNKR THEN 'СНКР '&#xA;&#x9;&#x9;&#x9;When  @SKK THEN 'СКК '&#xA;&#x9;&#x9;&#x9;When  @SKKR THEN 'СККР '&#xA;&#x9;&#x9;&#x9;When  @DO THEN 'ДО '&#xA;&#x9;&#x9;&#x9;When  @KO THEN 'КО '&#xA;&#x9;&#x9;&#x9;When  @KorDO THEN 'КорДО '&#xA;&#x9;&#x9;&#x9;When  @KorKO THEN 'КорКО '&#xA;&#x9;&#x9;&#x9;When  @DOKO THEN 'ДО-КО '&#xA;&#x9;&#x9;&#x9;When  @KorDOKorKO THEN 'КорДО-КорКО '&#xA;&#x9;&#x9;&#x9;Else '' END+RTRIM(LTRIM(ПланСчетов_Управленческий.Код))+'-'+LTRIM(RTRIM(ISNULL(ПланСчетов_Управленческий_К.Код,'')))+' '+LTRIM(RTRIM(ПланСчетов_Управленческий.Наименование)) Descr &#xA;   , ФинКоррСтатейВОтчетах.Счет schet&#xA;   , (SELECT КодОтчета FROM LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах ФинСтатьиВОтчетах WHERE ФинСтатьиВОтчетах.Ссылка=ФинКоррСтатейВОтчетах.Владелец) otchet&#xA;   ,'+'&#xA;FROM LSERVER63.s3_conv_full.dbo.Справочник_ФинКоррСтатейВОтчетах ФинКоррСтатейВОтчетах&#xA;INNER JOIN LSERVER63.s3_conv_full.dbo.Справочник_ФинСтатьиВОтчетах ФинСтатьиВОтчетах ON ФинКоррСтатейВОтчетах.Владелец = ФинСтатьиВОтчетах.Ссылка&#xA;INNER JOIN LSERVER63.s3_conv_full.dbo.Перечисление_ФинВидыИтогов ФинВидыИтогов ON ФинВидыИтогов.Ссылка = ФинКоррСтатейВОтчетах.ВидСуммы&#xA;LEFT JOIN LSERVER63.s3_conv_full.dbo.ПланСчетов_Управленческий ПланСчетов_Управленческий ON ПланСчетов_Управленческий.Ссылка=ФинКоррСтатейВОтчетах.Счет&#xA;LEFT JOIN LSERVER63.s3_conv_full.dbo.ПланСчетов_Управленческий ПланСчетов_Управленческий_К ON ПланСчетов_Управленческий_К.Ссылка=ФинКоррСтатейВОтчетах.КорСчет&#xA;WHERE ФинКоррСтатейВОтчетах.ПометкаУдаления = 0 AND ФинКоррСтатейВОтчетах.Активен = 1&#xA;order by ParentID&#xA;&#xA;DELETE FROM FinPeriods&#xA;INSERT INTO FinPeriods&#xA;SELECT DATEADD(yy,-2000,ФинПериодыОЛАП.начПериода),DATEADD(yy,-2000,ФинПериодыОЛАП.конПериода), ФинПериодыОЛАП.Наименование,ФинПериодыОЛАП.флОбновлять&#xA;FROM LSERVER63.s3_conv_full.dbo.Справочник_ФинПериодыОЛАП ФинПериодыОЛАП WHERE ФинПериодыОЛАП.ПометкаУдаления=0&#xA;&#xA;DELETE FROM FinScena WHERE ID &lt;&gt; @nullreference&#xA;INSERT INTO FinScena&#xA;SELECT &#xA;&#x9;ФинСценарии.Ссылка&#xA;&#x9;,ФинСценарии.Родитель&#xA;    ,ФинСценарии.Код&#xA;    ,ФинСценарии.ПометкаУдаления&#xA;FROM LSERVER63.s3_conv_full.dbo.Справочник_ФинСценарии ФинСценарии&#xA;union&#xA;Select  &#xA;&#x9;@nullreference&#xA;&#x9;,@nullreference&#xA;,'все сценарии'&#xA;,0x00&#xA;&#xA;FROM LSERVER63.s3_conv_full.dbo.Справочник_ФинСценарии ФинСценарии&#xA;DELETE FROM FinCFO&#xA;INSERT INTO FinCFO&#xA;SELECT ЦФО.Ссылка&#xA;     , ЦФО.Родитель&#xA;     , ЦФО.Наименование&#xA;     , ЦФО.ПометкаУдаления&#xA;     , ЦФО.Внешний&#xA;     , '+'&#xA;FROM LSERVER63.s3_conv_full.dbo.Справочник_ЦФО ЦФО&#xA;Union &#xA;&#x9;Select  @nullreference&#xA;&#x9;,@nullreference&#xA;&#x9;,'По всем ЦФО'&#xA;&#x9;,0x00&#xA;&#x9;,0&#xA;&#x9;,'+'  &#xA;FROM LSERVER63.s3_conv_full.dbo.Справочник_ЦФО ЦФО" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
